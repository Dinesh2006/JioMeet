<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite Jio">

	<listeners>
		<listener class-name="resources.listeners" />
		<listener class-name="resources.ExtentReporterNG" />
	</listeners>

	<test name="Validating Jio Meet Flow">
		<classes>
			<class name="tests.JioMeetTest">
				<methods>
					<exclude name="Login" />
					<exclude name="LoginError" />
					<exclude name="ManageAddUser" />
					<exclude name="ManageVideoUser" />
					<exclude name="Report" />
					<include name="Sanity" />
				</methods>
			</class>
		</classes>
	</test>

</suite> <!-- Suite -->
